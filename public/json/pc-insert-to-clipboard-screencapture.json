{
  "title": "Use PC Insert button for screen capture to Clipboard",
  "maintainers": [
    "unity1979"
  ],
  "rules": [
    {
      "description": "Insert to Control+Command+Shift+4 (Screencap to Clipboard)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "insert"
          },
          "to": {
            "key_code": "4",
            "modifiers": [
              "left_control",
              "left_command",
              "left_shift"
            ]
          }
        }
      ]
    }
  ]
}